def multipleof5(int a)
{
    if(a%5==0)              // Checing the modulous value
    {
        print "Hurray! " ^ a ^ " is multiple of 5 : )";
    }
    else 
    {
        print "Oh no! " ^ a ^ " is not multiple of 5 : (";
    }
}

